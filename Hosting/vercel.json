{
  "version": 2,
  "builds": [
    {
      "src": "Hosting/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Hosting/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "Hosting.settings.production"
  },
  "functions": {
    "Hosting/wsgi.py": {
      "maxDuration": 30
    }
  }
}
